<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<!--
  - Web Application Context definition for "SimpleInquest engine" DispatcherServlet.
  -->
<beans>
	<bean id="de.u808.simpleinquest"
         class="org.springframework.context.support.ClassPathXmlApplicationContext">
	     <constructor-arg>
	       <list>
	         <value>de/u808/simpleinquest/applicationContext.xml</value>
	       </list>
	     </constructor-arg>
   </bean>
</beans>
